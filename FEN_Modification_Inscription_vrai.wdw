         	HLitRecherchePremier(Eleve,IDEleve,COMBO_Eleve)
         	SI HTrouve(Eleve) = Vrai ALORS
         		HLitRecherchePremier(Statut_Eleve,IDStatut_Eleve,Eleve.Statut)
         		SI HTrouve(Statut_Eleve) ALORS
         			SI Statut_Eleve.Statut = "Formation Exonorée" ALORS
