         	HLitRecherchePremier(ClassesSchool,IDClassesSchool,COMBO_ClassesSchool)
         	SI HTrouve(ClassesSchool) ALORS
         		//	SAI_Montant_Inscription = ClassesSchool.Inscription 
         		HLitRecherchePremier(Eleve,IDEleve,COMBO_Eleve)
         		SI HTrouve(Eleve) = Vrai ALORS
         			HLitRecherchePremier(Statut_Eleve,IDStatut_Eleve,Eleve.IDStatut_Eleve)
         			SI HTrouve(Statut_Eleve) ALORS
         				SI Statut_Eleve.Statut = "Formation Exonorée" ALORS
         					SAI_Total_payé			=  0
         					SAI_Reste_a_payer		=  0
         					SAI_Montant_Inscription	= 0
         					SAI_Autrefrais			= 0
         					
         				SINON
         					SAI_Total_payé			=  ClassesSchool.total_a_payer 
         					SAI_Reste_a_payer		=   ClassesSchool.total_a_payer
         					SAI_Montant_Inscription	= ClassesSchool.Inscription
         					SAI_Autrefrais			= ClassesSchool.Autres_Frais
