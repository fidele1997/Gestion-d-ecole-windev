  -
    name : BTN_Valider1
    identifier : 0x22b0b39f0a0c8721
    internal_properties : 
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         SI SAI_Référence_récu = "" ALORS
         	Info("Veuillez entrer la référence du récu...")
         	RETOUR
         FIN
         
         SI SAI_annee_scolaire = "" ALORS
         	Info("Veuillez entrer l'année académique...")
         	RETOUR
         FIN
         
         SI COMBO_ClassesSchool = "" ALORS
         	Info("Veuillez sélectionner la classe...")
         	RETOUR
         FIN
         
         SI COMBO_Eleve = "" ALORS
         	Info("Veuillez sélectionner un élève...")
         	RETOUR
         FIN
         
         SI SAI_Montant_Inscription = "" ALORS
         	Info("Veuillez entré le montant d'inscription ...")
         	RETOUR
         FIN
         
         SI SAI_Date_d_inscription = "" ALORS
         	Info("Veuillez entré la date d'inscription ...")
         	RETOUR
         FIN
         HLitRecherchePremier(Eleve,IDEleve,COMBO_Eleve)
         SI HTrouve(Eleve) = Vrai ALORS
         	HLitRecherchePremier(Statut_Eleve,IDStatut_Eleve,Eleve.IDStatut_Eleve)
         	SI HTrouve(Statut_Eleve) ALORS
         		SI Statut_Eleve.Statut = "Formation Exonorée" ALORS
         			HLitRecherchePremier(Inscription,IDInscription,gIdinscription)
         			SI HTrouve(Inscription) ALORS
         				Inscription.reste_a_payer				= SAI_Reste_a_payer
         				Inscription.Tot_payé					= Val(SAI_Montant_Inscription) + Val(SAI_Scolarite) + Val(SAI_Autrefrais)
         				Inscription.Montant_Inscription			= SAI_Montant_Inscription
         				Inscription.date_inscription			= SAI_Date_d_inscription
         				Inscription.IDClassesSchool				= COMBO_ClassesSchool
         				Inscription.Classes						= COMBO_ClassesSchool.ValeurAffichée
         				Inscription.Autrefrais					= SAI_Autrefrais
         				Inscription.Eleve						= COMBO_Eleve..ValeurAffichée
         				Inscription.IDEleve						= COMBO_Eleve
         				Inscription.AnneAcademique				= SAI_annee_scolaire
         				Inscription.refrecu						= SAI_Référence_récu
         				Inscription.Scolarite					= SAI_Scolarite
         				Inscription.total_Formation				= SAI_Total_payé
         				Inscription.V1							= SAI_Scolarite
         				HModifie(Inscription)
         				Info("Modification effectuée avec succès...")
         //			Info("Inscription effectuée avec succès...")
         			Ferme()
         			fin
         		SINON
         			HLitRecherchePremier(Inscription,IDInscription,gIdinscription)
         			si HTrouve(Inscription) ALORS
         				Inscription.reste_a_payer				= SAI_Reste_a_payer
         				Inscription.Tot_payé					= Val(SAI_Montant_Inscription) + Val(SAI_Scolarite) + Val(SAI_Autrefrais)
         				Inscription.Montant_Inscription			= SAI_Montant_Inscription
         				Inscription.date_inscription			= SAI_Date_d_inscription
         				Inscription.IDClassesSchool				= COMBO_ClassesSchool
         				Inscription.Classes						= COMBO_ClassesSchool.ValeurAffichée
         				Inscription.Autrefrais					= SAI_Autrefrais
         				Inscription.Eleve						= COMBO_Eleve..ValeurAffichée
         				Inscription.IDEleve						= COMBO_Eleve
         				Inscription.AnneAcademique				= SAI_annee_scolaire
         				Inscription.refrecu						= SAI_Référence_récu
         				Inscription.Scolarite					= SAI_Scolarite
         				Inscription.total_Formation				= SAI_Total_payé
         				Inscription.V1							= SAI_Scolarite
         				HModifie(Inscription)
         				info("Modification effectuée avec succès...")
         				Ferme()
         			fin
         		fin
         	fin
         FIN
        type : 18
