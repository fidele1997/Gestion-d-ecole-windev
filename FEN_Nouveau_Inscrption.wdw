         				Recouvrement.ref_reçu				= SAI_Référence_récu
         				Recouvrement.Reste_Paye				= Val(nVinscription) - Val(SAI_Autrefrais)
         				nVautre_frais						= Val(nVinscription) - Val(SAI_Autrefrais)
         				HAjoute(Recouvrement)
