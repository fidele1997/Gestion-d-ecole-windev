         				Recouvrement.TRANCHE_PAIEMENT	= "1ère Tranche"
         				Recouvrement.Montant_du			= nVautre_frais
         				Recouvrement.Mont_paye			= SAI_Scolarite
         				Recouvrement.ref_reçu			= SAI_Référence_récu
         				Recouvrement.Reste_Paye			= Val(nVautre_frais) - Val(SAI_Scolarite)
         				nV1tranche						= Val(nVautre_frais) - Val(SAI_Scolarite)
         				HAjoute(Recouvrement)
         				HLitRecherchePremier(parametre_logiciel,lib,"INSCRIPTION")
         				SI HTrouve(parametre_logiciel) ALORS
         					parametre_logiciel.valeur = Val(parametre_logiciel.valeur) + 1
         					HModifie(parametre_logiciel)
         				FIN
         				info("Inscription effectuée avec succès...")
         				ferme()
