    name : BTN_Valider1
    identifier : 0x2298785a02c1fec0
    internal_properties : 
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         nVinscription, nV1tranche, nVautre_frais est un entier
         SI SAI_Référence_récu = "" ALORS
         	Info("Veuillez entrer la référence du récu...")
         	RETOUR
         FIN
         
         SI SAI_annee_scolaire = "" ALORS
         	Info("Veuillez entrer l'année académique...")
         	RETOUR
         FIN
         SI COMBO_ClassesSchool = "" ALORS
         	Info("Veuillez sélectionner la classe...")
         	RETOUR
         FIN
         
         SI COMBO_Eleve = "" ALORS
         	Info("Veuillez sélectionner un élève...")
         	RETOUR
         FIN
         
         SI SAI_Montant_Inscription = "" ALORS
         	Info("Veuillez entré le montant d'inscription ...")
         	RETOUR
         FIN
         
         SI SAI_Date_d_inscription = "" ALORS
         	Info("Veuillez entré la date d'inscription ...")
         	RETOUR
         FIN
         HSupprimetout(Etat_Recu)
         HExécuteRequête(REQ_Verification_Inscription_vrai,hRequêteDéfaut,SAI_annee_scolaire,null,null,null,COMBO_Eleve,null)
         HLitPremier(REQ_Verification_Inscription_vrai)
         si pas HEnDehors(REQ_Verification_Inscription_vrai) alors
         	info("Enregistrement impossible! Cet apprenant s'est déjà inscrit dans la formation: " ,REQ_Verification_Inscription_vrai.Classee)
         	retour
         FIN
         
         
         HLitRecherchePremier(Inscription,refrecu,SAI_Référence_récu)
         si HTrouve(Inscription) ALORS
         	Info("Inscription existe déjà...")
         	retour
         sinon
         	HLitRecherchePremier(Eleve,IDEleve,COMBO_Eleve)
         	SI HTrouve(Eleve) = Vrai ALORS
         		HLitRecherchePremier(Statut_Eleve,IDStatut_Eleve,Eleve.IDStatut_Eleve)
         		SI HTrouve(Statut_Eleve) ALORS
         			HExécuteRequête(REQ_verification_exclut,hRequêteDéfaut,Eleve.IDEleve,COMBO_ClassesSchool,SAI_annee_scolaire)
         			HPremier(REQ_verification_exclut)
         			SI PAS HEnDehors(REQ_verification_exclut) ALORS
         				Info("Cet apprenant est déjà exclu....","Inscription impossible")
         				retour
         			FIN
         			SI Statut_Eleve.Statut = "Formation Exonorée" ALORS
         				Inscription.reste_a_payer				= SAI_Reste_a_payer
         				Inscription.Tot_payé					= Val(SAI_Montant_Inscription) + Val(SAI_Scolarite) + Val(SAI_Autrefrais)
         				Inscription.Montant_Inscription			= SAI_Montant_Inscription
         				Inscription.date_inscription			= SAI_Date_d_inscription
         				Inscription.IDClassesSchool				= COMBO_ClassesSchool
         				Inscription.Classes						= COMBO_ClassesSchool.ValeurAffichée
         				Inscription.Autrefrais					= SAI_Autrefrais
         				Inscription.Eleve						= COMBO_Eleve..ValeurAffichée
         				Inscription.IDEleve						= COMBO_Eleve
         				Inscription.AnneAcademique				= SAI_annee_scolaire
         				Inscription.refrecu						= SAI_Référence_récu
         				Inscription.Scolarite					= SAI_Scolarite
         				Inscription.total_Formation				= SAI_Total_payé
         				Inscription.V1							= SAI_Scolarite
         				HAjoute(Inscription)
         				Recouvrement.AnneAcademique		= SAI_annee_scolaire
         				Recouvrement.IDEleve			= COMBO_Eleve
         				Recouvrement.IDClassesSchool	= COMBO_ClassesSchool
         				Recouvrement.Eleve				= COMBO_Eleve..ValeurAffichée
         				Recouvrement.Classee			= COMBO_ClassesSchool..ValeurAffichée
         				Recouvrement.Date_recouvrement	= SAI_Date_d_inscription
         				Recouvrement.TRANCHE_PAIEMENT	= "Inscription"
         				Recouvrement.Montant_du			= SAI_Total_payé
         				Recouvrement.Mont_paye			= SAI_Montant_Inscription
